{
    "manifest_version": 2,
    "name": "SNL darkmode",
    "version": "0.2",
    "description": "Mørk modus for Store norske Leksikon, så du kan researche hele natta!",
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["darkmodeHandler.js"]
        }
    ],
    "permissions": ["activeTab"],
    "web_accessible_resources": [
        "darkModes.json"
    ]
}